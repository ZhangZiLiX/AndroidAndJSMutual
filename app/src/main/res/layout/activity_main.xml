<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <WebView
        android:id="@+id/webview"
        android:layout_width="match_parent"
        android:layout_height="200dp">

    </WebView>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        >

        <TextView
            android:id="@+id/txt_acalljs"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:textColor="#f00"
            android:textSize="25sp"
            android:background="#B4A9A9"
            />

        <Button
            android:id="@+id/btn_alert_androidcalljs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#0D30B3"
            android:text="警告弹框：android调用本地JS代码"
            />

        <Button
            android:id="@+id/btn_prompt_androidcalljs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#0D30B3"
            android:text="提示弹框：android调用本地JS代码"
            />

        <Button
            android:id="@+id/btn_confirm_androidcalljs"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#0D30B3"
            android:text="确认弹框：android调用本地JS代码"
            />


    </LinearLayout>

</LinearLayout>